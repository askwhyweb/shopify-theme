  @media only screen and (max-width: 767px) {
    input#footercheck {
      position: relative;
      top: 2px;
    }
    input#footercheckcontact {
      top: 0px;
      position: relative;
    }
    input#footerchecka {
      position: relative;
      top: 2px;
    }
    input#footercheckcontactn {
