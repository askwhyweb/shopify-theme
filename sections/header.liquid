  {% if settings.enable_header_promotion and settings.optionofphone == 'Menu' %}
  .contact {
    float: left;
}
   header .contact{display:block}
  header.header.shrink .contact{display:block}
  {% endif %}
   {% if settings.product_layout_option == "product_four" %}
