<header class="header container-fluid">
  {% if settings.enable_topcbar %}
    {% if settings.coupan != '' %}
      <div class="row top_bar_save">	
        <div class="container">
          <p class='coupon-text'>Coupon Code: <span id="coupan_code_copy" >{{ settings.coupan }}</span> <span class="button_copy_code">Copy Code</span></p>
        </div>
      </div>
    {% endif %}
  {% endif %}
  <div class="row top_bar {% if settings.enable_sticky_headerr == "two" %}no-border-sticky{% endif %}">	
    <div class="container table--layout">
         
        {% if settings.enable_header_fb_page %}
          {% if settings.header_fb_page_url != "" %}
            <div class="fblikebar">
              <div class="cbha">
                <div id="fb-root"></div>
                <div class="fb-like" data-href="{{ settings.header_fb_page_url }}" data-layout="button_count" data-action="like" data-size="small" data-show-faces="false" data-share="true"></div>
              </div>
            </div>
          {% endif %}
        {% endif %}
    

      {% if settings.enable_strip_text %}
      <div class="shipping_bar">
        <p>{% if settings.strip_url != "" %}<a style="color: inherit;" class="free_shipping_top_strip" href="{{settings.strip_url}}">{% else %}<a style="color: inherit;" class="free_shipping_top_strip" href="javascript:void(0);">{% endif %} {% if settings.strip_url != "" %} </a> {% else %} </a> {% endif %}{% if settings.strip_display_user_country %}<span class="countryName"></span>&nbsp;<span class="flagImg f32"></span>{% endif %}</p>
      </div>
      {% endif %}
      <nav class="main-nav topnav">
         <ul class="tpn">
          {% if settings.show_multiple_currencies %}
          <li class="dropdown">
            {% include 'currencies-switcher' %}
          </li>
          {% endif %}
         
          {% for link in linklists[settings.strip_linklist].links %}
          <li><a href="{{link.url}}">{{link.title}}</a></li>
          {% endfor %}
           {% if shop.customer_accounts_enabled %}
          <li><a href="/account">{{ 'customer.account.title' | t }}</a></li>
          {% if customer %}
          <li><a href="/account/logout">{{ 'layout.customer.log_out' | t }}</a></li>
          {% endif %}
            {% endif %}
        </ul>
        <div class="cart nm">
          {% if template !="cart" %}
          <li class="top-nav-link cart__hover">
            <a  data-no-instant href="/cart" class="top-nav-link site-header__cart-toggle js-drawer-open-right" aria-controls="CartDrawer" aria-expanded="false">
              {% if settings.usecutomiconforcart == true %}
              <img src="{{'cart.png' | asset_url}}" class="cartimga">
              {% else%}
              <i class="fa fa-shopping-cart"></i>
              {% endif %}
              <span class="CartCount">{{ cart.item_count }}</span>
            </a>
            {% if cart.item_count > 0 %}
            <div id="cart-popup">
              <div class="container">
                <div class="cartForm">
                  <ul class="cart-popup-ul-middle">
                    {% for item in cart.items %}
                    <li>
                      <a href="{{ item.url | within: collections.all }}">
                        <div class="cart-img-div">
                          <img src="{{ item | img_url: 'small' }}" alt="Image of {{ item.title | escape }}" class="cart-item-image" />
                          <span class="pop-qty-crat">{{ item.quantity }}</span>
                        </div>
                        <div class="cart-item-info">
                          <span class="cart-item-title">
                            {{ item.product.title }}{% if item.product.variants.size > 1 %} - {{ item.variant.title }}{% endif %}
                            {% if settings.product_personalisation %}
                            {% assign property_size = item.properties | size %}
                            {% if property_size > 0 %}
                            {% for p in item.properties %}
                            {% unless p.last == blank %}
                            {{ p.first }}:
                            {% if p.last contains '/uploads/' %}
                            <a class="lightbox" href="{{ p.last }}">{{ p.last | split: '/' | last }}</a>
                            {% else %}
                            {{ p.last }}
                            {% endif %}
                            <br>
                            {% endunless %}
                            {% endfor %}
                            {% endif %}
                            {% endif %}
                          </span>
                        </div>
                      </a>
                      <a  data-no-instant href="/cart/change?line={{ forloop.index }}&quantity=0" data-line="{{ forloop.index }}" data-variant="{{ item.variant.id }}" class="removeLineCartPop" rel="{{ item.variant.id }}"><i class="icon icon-x"></i></a>
                    </li>
                    {% endfor %}
                  </ul>
                  <ul class="cart-popup-ul-top">
                    <li class="total-items"><b>{{ 'cart.general.total' | t }}: </b><span class="cart-current-items">{{ cart.item_count }}</span> {{ cart.item_count | pluralize: 'Item', 'Items' }}</li>
                    <li class="total-price">{{ cart.total_price | money }}</li>
                  </ul>
                  <ul class="cart-popup-ul-bottom">
                    <li><a href="/cart" class="btn--secondary btn--full cart__cartview">{{ 'cart.general.button' | t }}</a></li>
                    <li>
                      <a href="/checkout" class="btn btn--full cart__checkout Checkout">
                        <img src="{{ 'checkout-button-icon.png' | asset_url }}" alt="{{ shop.name }}" itemprop="logo" class="no-border ratina-img addIcon" />
                        <span id="CheckoutText">{{ 'cart.general.checkout' | t }}</span>
                      </a>
                      {% if settings.enable_cartstrip_text %}
                        <div class="cart--promo-msg common_shipping_text"></div>
                      {% endif %}
                    </li>
                  </ul>
                </div>
              </div>
            </div>
            {% else %}
            <div id="cart-popup" class="empty-popup">
              <div class="container">
                <ul class="cart-popup-ul-bottom">
                  <li class="empty-cart-popup-msg">{{ 'cart.general.empty' | t }}</li>
                  <li><a href="/collections/all" class="btn btn-large btn-primary">{{ 'cart.general.shop_now' | t }}</a></li>
                </ul>
              </div>
            </div>
            {% endif %}
          </li>
          {% endif %}
        </div>
      </nav>
    </div>
  </div>
  
  <div class="row middle_bar">
    {% if settings.enable_mobile_topbar %}
    <div class="top--line mshop"><span class="mobile_bar_shipping_text">{{ settings.mobile_top_strip }}</span>{% if settings.enable_banner_user_country %}&nbsp;<span class="flagImg f32"></span>&nbsp;<span class="countryName"></span>{% endif %}</div>
    {% endif %}
    <div class="container" {% if template =="cart" %}style="height: 50px;"{% endif %}>
      <div class="menu-btn {% if template =="cartt" %}medium-down--hide large--hide{% endif %}">
        <a href="#menu"><span class="fa fa-bars"></span></a>
      </div>
      <div class="search {% if template =="cartsearcht" %}medium-down--hide large--hide{% endif %}">
        <a href="javascript:;" class="header-search" id="searchlink">
          <span class="fa fa-search"></span>
        </a>
      </div>
      <div class="noproclas col-md-3">
        <div class="row">
          <div class="logo">
            {% if settings.logo_use_image %}
              <a href="/" itemprop="url" class="desktop-logo">
                <img src="{{ 'logo.png' | asset_url }}" alt="{{ shop.name }}" itemprop="logo" class="img-responsive" height="70" {% if settings.logo_max_width != ''%} style="width:{{settings.logo_max_width}}px" {% endif%}/>
              </a>
            {% else %}
              <a href="/" itemprop="url">{{ shop.name }}</a>
            {% endif %}
            <a href="/" class="mobile-logo">
              <img src="{{ 'mobile_logo.png' | asset_url }}" class="img-responsive" height="28" width="145">
            </a>
          </div>
        </div>
      </div>
      <div class="{% if settings.enable_header_promotion %}col-md-4 {% else %}col-md-6 {% endif %}">
        {% if settings.enable_header_search %}
        {% if template !="cart" %}  {% endif %}
        <div class="search_panel">

          {% include 'search-bar' %}
        </div>
        {% endif %}
      </div>
     
        {% if settings.enable_header_promotion %}
          <div class="col-md-2 mob-hide">
          
            <div class="header-promotion-image">               
              {% if settings.header_promotion_link != ''%}
              <a href="{{settings.header_promotion_link}}">
                {% endif %}
                <img src="{{ 'transparent.png' | asset_url }}"
                     data-src="{{ 'header_image.png' | asset_url }}"
 data-widths="[540, 720, 900, 1080, 1296, 1512, 1728, 1944, 2048]"
 data-aspectratio="{{ 'header_image.png' | asset_url }}"
 data-sizes="auto"
 data-parent-fit="contain"
                     class="lazyload"
                     alt="Image of {{ shop.name | escape }}" />
                {% if settings.header_promotion_link != ''%}
              </a>
              {% endif %}
            </div>
         
          </div>
        {% endif %}
      
      {% if settings.enable_header_contact %}
        <div class="col-md-3 mob-hide">
          <div class="contact">
            {% if settings.optionofphone == 'Menu' %}
            
            {% else %}
              <p class="hsit">{{ settings.header_question_line }}</p>
              <h5><a href="tel:{{ settings.header_phone }}">
                {% if settings.usecustomphoneicon == true %}
                <img src="{{'transparent.png' | asset_url}}" 
                     data-src="{{'phone-icon.png' | asset_url}}"
 data-widths="[540, 720, 900, 1080, 1296, 1512, 1728, 1944, 2048]"
 data-aspectratio="{{'phone-icon.png' | asset_url}}"
 data-sizes="auto"
 data-parent-fit="contain"
                     class="lazyload picon">
                {% else %}
                <span class="fa fa-phone">
                  {% endif %}
                </span>{{ settings.header_phone }} </a></h5>
              <p><strong>{{ settings.header_time }}</strong></p>
            {% endif %}
          </div>
        </div>
      {% endif %}
      <div class="cart cart2 {% if template =="cart" %}medium-down--hide large--hide{% endif %}" {% if template =="cart" %}style="display: none !important;"{% endif %}>
        <a  data-no-instant href="/cart" class="top-nav-link site-header__cart-toggle js-drawer-open-right" aria-controls="CartDrawer" aria-expanded="false">
          {% if settings.usecutomiconforcart == true %}
              <img src="{{'cart.png' | asset_url}}" class="cartimga">
              {% else%}
          <i class="fa fa-shopping-cart"></i>
          {% endif %}
          <span class="CartCount">{{ cart.item_count }}</span>
        </a> 
      </div>
       {% if settings.show_multiple_currencies %}
      <div class="mobile-currencies {% if template =="cart" %}medium-down--hide large--hide{% endif %}">
        <div class="dropdown">
          {% include 'currencies-switcher' %}
        </div>
      </div>
      {% endif %}
    </div>
  </div>
  <div class="row menu_bar">
    <div class="container">
      <div class="menu">
        <ul>
          {%include 'navigation'%}
        </ul>
      </div>
      {% if settings.enable_header_contact %}
        <div class="menu-right">
          <ul>
            <li class="header_time"> {{ settings.header_time }}</li>
            <li class="header_phone"><a href="tel:{{ settings.header_phone }}">
              
                {% if settings.usecustomphoneicon == true %}
            <img src="{{'phone-icon.png' | asset_url}}" class="picona">
            {% else %}
              <span class="fa fa-phone"></span>
            {% endif %}
              
              {{ settings.header_phone }} </a></li>
          </ul>
        </div>
      {% endif %}
    </div>
  </div>
  {% assign stickyEnable = false %}
  {% if settings.sticky_bar_method == "site" %}
  	{% assign stickyEnable = true %}
  {% else %}
  	{% if template == "product" %}
  	  {% for tag in product.tags %}
  		{% if tag == "sticky-header-bar" %}{% assign stickyEnable = true %}{% endif %}
  	  {% endfor %}
  	{% endif %}
  {% endif %}
  {% if stickyEnable %}
    {% if settings.enable_sticky_bar or settings.enable_sticky_bar_mobile %}
      {% assign stickybarenable = "" %}
      {% assign stickybarenableinmobile = "" %}
      {% if settings.enable_sticky_bar == false %}
        {% assign stickybarenable = " large--hide" %}
      {% endif %}
      {% if settings.enable_sticky_bar_mobile == false %}
        {% assign stickybarenableinmobile = " medium-down--hide" %}
      {% endif %}
      <div class="sticky_bar{{ stickybarenable }}{{ stickybarenableinmobile }}">
        <div class="popup-header-bar sticky_bar_text"></div>&nbsp;
        {% if settings.enable_sticky_bar_timer %}
          <div class="sticky_bar_timer" data-cutoff="{{ settings.sticky_bar_timer_cutoff }}" data-reset="{{ settings.sticky_bar_reset }}">
            <span class="days"></span> : <span class="hours"></span> : <span class="minutes"></span> : <span class="seconds"></span>
          </div>
        {% endif %}
      </div>
    {% endif %}
  {% endif %}
</header>
